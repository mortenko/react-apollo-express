{
  "printWidth": 80,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "none",
  "overrides": [
    {
      "files": "*.scss",
      "options": { "parser": "scss" }
    },
    {
      "files": "*.js",
      "options": { "parser": "babel" }
    },
    {
      "files": "*.json",
      "options": { "parser": "json" }
    }
  ]
}
