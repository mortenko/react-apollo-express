    "classnames": "^2.2.6",
    "cors": "^2.8.4",
    "date-fns": "^1.29.0",
    "dotenv": "4.0.0",
    "express": "^4.16.3",
    "fs-extra": "3.0.1",
    "graphql": "^0.13.2",
    "graphql-type-json": "^0.2.1",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "normalize.css": "^8.0.0",
    "pg": "6.4.2",
    "pg-hstore": "^2.3.2",
    "promise": "^8.0.1",
    "promise-polyfill": "8.1.0",
    "query-string": "^6.1.0",
    "react": "^16.6.3",
    "react-apollo": "^2.3.1",
    "react-dev-utils": "^7.0.0",
    "react-dom": "^16.6.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-scroll": "^0.4.4",
    "recompose": "^0.30.0",
    "sequelize": "^4.37.10",
    "sequelize-cli": "^5.1.0",
    "uuid": "^3.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "1.1.3",
    "css-loader": "^1.0.0",
    "dataloader": "^1.4.0",
    "eslint": "^5.7.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-graphql": "^2.1.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "faker": "^4.1.0",
    "file-loader": "^2.0.0",
    "jest": "23.6.0",
    "jest-cli": "^23.6.0",
    "lodash": "^4.17.4",
    "mini-css-extract-plugin": "^0.4.2",
    "morgan": "^1.9.0",
    "node-sass-chokidar": "^1.3.3",
    "nodemon": "^1.18.4",
    "postcss-flexbugs-fixes": "^4.1.0",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "react-hot-loader": "^4.3.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "stylelint": "^9.6.0",
    "stylelint-scss": "^3.3.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-dev-server": "^3.1.5",
    "webpack-manifest-plugin": "^2.0.3"
  },
  "scripts": {
    "start-dev": "NODE_ENV=development node scripts/start.js",
    "build": "NODE_ENV=production node scripts/build.js",
    "start-server": "NODE_ENV=development nodemon --exec babel-node src/graphql-api/server/index.js",
    "test": "node scripts/test.js --env=jsdom",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo:all",
    "db:seed": "sequelize db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo:all",
    "start-postgres": "brew services start postgresql",
    "shut-down-postgres": "brew services stop postgresql",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{j@babel/preset-stage-3s,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
